{
  "$schema": "./data.schema.json",
  "version": "1.3.0",
  "parsers": [
    {
      "package": "@babel/eslint-parser",
      "tags": [
        "babel"
      ]
    },
    {
      "package": "@graphql-eslint/eslint-plugin",
      "tags": [
        "graphql"
      ]
    },
    {
      "package": "@html5-eslint/parser",
      "tags": [
        "html"
      ]
    },
    {
      "package": "@typescript-eslint/parser",
