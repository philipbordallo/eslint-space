      "package": "@graphql-eslint/eslint-plugin",
      "parserNeeded": "@graphql-eslint/eslint-plugin",
      "rules": [
        {
          "docsURL": "https://github.com/B2o5T/graphql-eslint/blob/master/docs/rules/:rule.md",
          "path": "@graphql-eslint/:rule"
        }
      ],
      "tags": [
        "graphql"
      ]
    },
    {
      "package": "@html-eslint/eslint-plugin",
      "parserNeeded": "@html5-eslint/parser",
      "rules": [
        {
          "docsURL": "https://github.com/yeonjuan/html-eslint/blob/main/docs/rules/:rule.md",
          "path": "@html-eslint/:rule"
        }
      ],
      "tags": [
        "html"
      ]
    },
    {
      "package": "@typescript-eslint/eslint-plugin",
      "parserNeeded": "@typescript-eslint/parser",
      "rules": [
        {
